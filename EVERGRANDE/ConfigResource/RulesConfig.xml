<?xml version="1.0" encoding="utf-8" ?>
<Rules>
<!--条码规则的先后顺序，首先位数限制，其次是条码含有某些字符的规则或者是过滤字符，最后是截取-->
  <Rule>
    <Key>PN</Key>
    <RegexValue>^.*$</RegexValue>
    <ErrorMessage>PN不能为空。</ErrorMessage>
    <Description>任何条码</Description>
    <Seq>1</Seq>
  </Rule>
  <Rule>
    <Key>PN</Key>
    <RegexValue>^.{11}|^.{10}</RegexValue>
    <ErrorMessage>条码长度出错。</ErrorMessage>
    <Description>长度要超过10位</Description>
    <Seq>2</Seq>
  </Rule>
  <Rule>
    <Key>PN</Key>
    <RegexValue>(?i)(?(?=p)^.{11}|^.{10})</RegexValue>
    <ErrorMessage>条码格式出错。</ErrorMessage>
    <Description>取全部情况出来</Description>
    <Seq>3</Seq>
  </Rule>
  <Rule>
    <Key>PN</Key>
    <RegexValue>.{10}$</RegexValue>
    <ErrorMessage>截取条码出错。</ErrorMessage>
    <Description>从后往前截取10位,最终取得比较的部分</Description>
    <Seq>4</Seq>
  </Rule>
  <Rule>
    <Key>Contrast</Key>
    <RegexValue>^.*$</RegexValue>
    <ErrorMessage>条码不能为空。</ErrorMessage>
    <Description>任何条码</Description>
    <Seq>1</Seq>
  </Rule>
  <Rule>
    <Key>Contrast</Key>
    <RegexValue>^.{11}$|^.{10}$</RegexValue>
    <ErrorMessage>条码长度出错。</ErrorMessage>
    <Description>长度要超过10位</Description>
    <Seq>2</Seq>
  </Rule>
  <Rule>
    <Key>Contrast</Key>
    <RegexValue>(?i)(?(?=p)^.{11}$|^.{10}$)</RegexValue>
    <ErrorMessage>条码格式出错。</ErrorMessage>
    <Description>取全部情况出来</Description>
    <Seq>3</Seq>
  </Rule>
  <Rule>
    <Key>Contrast</Key>
    <RegexValue>.{10}$</RegexValue>
    <ErrorMessage>截取条码出错。</ErrorMessage>
    <Description>从后往前截取10位,最终取得比较的部分</Description>
    <Seq>4</Seq>
  </Rule>
</Rules>
